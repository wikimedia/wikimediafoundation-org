{
  "name": "wmfoundation",
  "description": "Theme and plugins for WikiMedia Foundation project",
  "keywords": [],
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/reaktivstudios/wmfoundation",
  "authors": [
    {
      "name": "reaktivstudios",
      "homepage": "http://reaktivstudios.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/reaktivstudios/wmfoundation/issues",
    "source": "https://github.com/reaktivstudios/wmfoundation"
  },
  "config": {
    "sort-order": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
